							"typeProperties": {
								"variableName": "sep_id_column",
								"value": {
									"value": "@item()['ID Nation']",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Set variable2",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "Append variable1",
									"dependencyConditions": [
