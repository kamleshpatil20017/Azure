									]
								}
							],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "final_op_col_id",
								"value": {
									"value": "@variables('sep_id_column')",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"variables": {
